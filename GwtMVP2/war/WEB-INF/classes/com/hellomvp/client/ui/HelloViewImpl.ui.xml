<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		Hello,
		<span class="{style.important}" ui:field="nameSpan" />
		<g:Anchor ui:field="goodbyeLink" text="Say good-bye"></g:Anchor>
		<g:Button ui:field="generateRandomNumberButton" text="Generate A Random Number"> </g:Button>
		<g:Anchor ui:field="sameRandomNumber" text="Same Raondom Number"></g:Anchor>
		<g:TextBox ui:field="userName" ></g:TextBox>
		<g:TextBox ui:field="password"></g:TextBox>
		<g:Button ui:field="LoginButton" text="Login"></g:Button>
	</g:HTMLPanel>
</ui:UiBinder>